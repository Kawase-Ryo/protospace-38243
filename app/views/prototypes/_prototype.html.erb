<div class="card">

  <%= link_to image_tag(prototype.image, class: :card__img), prototype_path(prototype.id),method: :get%>
  <%#= link_to image_tag("プロトタイプの画像", class: :card__img ), root_path%>

  <div class="card__body">

   <%= link_to prototype.title, prototype_path(prototype.id),method: :get, class: :card__title%>
    <%#= link_to "プロトタイプのタイトル", root_path, class: :card__title%>
    <p class="card__summary">
      <%#= "プロトタイプのキャッチコピー" %>
    <%= link_to prototype.catch_copy, root_path(prototype.id)%>
    </p>

    <%= link_to "by #{prototype.user.name}", user_path(prototype.user), class: :card__user %>
    <%#= link_to "by プロトタイプの投稿者名", root_path, class: :card__user %>
  </div>
</div>